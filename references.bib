@article{cestnik_integrability_2024,
	title = {Integrability of a {Globally} {Coupled} {Complex} {Riccati} {Array}: {Quadratic} {Integrate}-and-{Fire} {Neurons}, {Phase} {Oscillators}, and {All} in {Between}},
