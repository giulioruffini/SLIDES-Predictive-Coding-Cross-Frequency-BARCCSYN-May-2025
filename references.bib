@article{pictonMismatchNegativityDifferent2000,
	title = {Mismatch {Negativity}: {Different} water in the same river},
	volume = {5},
	abstract = {Abstract The mismatch negativity (MMN) is a frontal negative deflection in the human event-related potential that typi- cally occurs when a repeating auditory stimulus changes in some manner. The MMN can be elicited by many kinds of stimulus change, varying from simple changes in a single stimulus feature to abstract changes in the relationship between stimuli. The main intracerebral sources for the MMN are located in the auditory cortices of the temporal lobe. Since it occurs whether or not stim- uli are being attended, the MMN represents an automatic cerebral process for detecting change. The MMN is clini- cally helpful in terms of demonstrating disordered sen- sory processing or disordered memory in groups of patients. Improvements in the techniques for measuring the MMN and in the paradigms for eliciting it will be needed before the MMN can become clinically useful as an objective measurement of such disorders in individu- al patients.},
